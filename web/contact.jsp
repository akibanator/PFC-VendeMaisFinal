                        <div class="content-main contact-content">
                            <div class="contact-content-upper">
                                <div class="row">
                                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                        <div class="gallery_title">
                                            <h3>VENDEMAIS</h3>
                                            <h4>CONTATO</h4>
                                        </div>    
                                    </div>                            
                                </div>

                                <div class="row">  
                                    <div class="col-sm-12 col-md-6 contact_left">        
                                        <form class="form-horizontal" method="post" action="#">

                                            <div class="form-group">                                    
                                                <input type="text" class="form-control" id="name" name="nome" placeholder="NOME COMPLETO..." value="" required>                                    
                                            </div>

                                            <div class="form-group">
                                                <input type="email" class="form-control" id="email" name="email" placeholder="EMAIL..." value="" required>      
                                            </div>

                                            <div class="form-group">
                                                <input type="telefone" class="form-control" id="telefone" name="telefone" placeholder="TELEFONE..." value="" required>      
                                            </div>

                                            <div class="form-group">
                                                <textarea class="form-control" rows="4" name="mensagem" placeholder="MENSAGEM..." required></textarea>
                                            </div>

                                            <div class="form-group">
                                                <p align="center"><input id="submit" name="submit" type="submit" value="Enviar" class="btn view_more btn-submit"></p>
                                            </div>            

                                        </form>    
                                    </div> <!-- .contact-left -->

                                    <div class="col-sm-12 col-md-6 contact_right">

                                        <div class="col-md-12 contact_title">
                                            CONTATO
                                        </div>

